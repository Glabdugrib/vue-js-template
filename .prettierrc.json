{
  "$schema": "https://json.schemastore.org/prettierrc",
  "attributeGroups": [
    "$DEFAULT",
    "^#",
    "^(@|v-on)",
    "^:?class$",
    "^:?data-",
    "^:?id$",
    "^:?is$",
    "^:?key$",
    "^:?ref$",
    "^:(?!data-)",
    "^(prop|[a-zA-Z0-9:]+-prop)$",
    "^(v-bind:prop|:prop)$",
    "^boolean-prop$",
    "^v-bind$",
    "^v-cloak$",
    "^v-for$",
    "^v-html$",
    "^v-model$",
    "^v-model:",
    "^v-once$",
    "^v-pre$",
    "^v-show$",
    "^v-text$",
    "^v-(if|else-if|else)$",
    "^v-(?!on\\b|text\\b|html\\b)[a-zA-Z0-9:]+$",
    "^v-html$",
    "^v-text$"
  ],
  "attributeSort": "ASC",
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-organize-attributes"],
  "printWidth": 100,
  "singleAttributePerLine": true,
  "trailingComma": "none",
  "useTabs": true
}
