{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "none",
  "plugins": ["prettier-plugin-tailwindcss"],
  "attributeGroups": [
    "^#",
    "^:?is$",
    "^v-for$",
    "^v-(if|else-if|else)$",
    "^v-show$",
    "^v-cloak$",
    "^v-pre$",
    "^v-once$",
    "^:?id$",
    "^:?ref$",
    "^:?key$",
    "^v-model$",
    "^v-model:",
    "^v-(?!on\b|text\b|html\b)[a-zA-Z0-9:]+$",
    "^(v-bind:prop|:prop)$",
    "^(prop|[a-zA-Z0-9:]+-prop)$",
    "^boolean-prop$",
    "^:?class$",
    "^:(?!data-)",
    "$DEFAULT",
    "^:?data-",
    "^(@|v-on)",
    "^v-html$",
    "^v-text$",
    "^v-bind$"
  ],
  "attributeSort": "ASC"
}